#include <stdio.h> 

int main() {
  int n, i, m = 0;
  printf("Enter any number n:");
  scanf("%d", &n);
  for (i = 1; i <= n; i++) {
      if (n % i == 0) {
         m++;
      }
  }

  if (m == 2) {
        printf("a Prime number");
  }
  else {
         printf("not a Prime number");
  }
  return 0;    
}
